{
    "name": "template",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "devDependencies": {
        "@types/bun": "latest",
        "dprint": "^0.49.1",
        "oxlint": "^0.16.6"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "scripts": {
        "lint": "bun oxlint",
        "dev": "bun index.html",
        "fmt": "bun dprint fmt",
        "typecheck": "bun tsc --noEmit",
        "submit": "bun fmt && git add . && git commit -m submit && git push"
    },
    "bun-create": {
        "postinstall": "rm -rf .git node_modules"
    }
}
