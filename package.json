{
    "name": "template",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "devDependencies": {
        "@types/bun": "latest",
        "dprint": "^0.49.1",
        "oxlint": "^0.16.6"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "scripts": {
        "lint": "bun install && bun oxlint",
        "dev": "bun --watch index.html",
        "fmt": "bun install && bun dprint fmt",
        "typecheck": "bun install && tsc --noEmit",
        "submit": "bun fmt && git add . && git commit -m submit && git push"
    }
}
